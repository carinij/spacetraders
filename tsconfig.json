{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "preserveConstEnums": true,
    "target": "ES5"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts", "**/titlescreen.ts"]
}